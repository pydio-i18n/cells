    }
    .content-cell {
      padding: 26pt;
    }
    .align-right {
      text-align: right;
    }

    h1 {
      margin-top: 0;
      color: #2F3133;
      font-size: 14pt;
      font-weight: bold;
    }
    h2 {
      margin-top: 0;
      color: #2F3133;
      font-size: 12pt;
      font-weight: bold;
    }
    h3 {
      margin-top: 0;
      color: #2F3133;
      font-size: 11pt;
      font-weight: bold;
    }
    blockquote {
      margin: 19pt 0;
      padding-left: 10px;
      border-left: 10px solid #F0F2F4;
    }
    blockquote p {
        font-size: 1.1rem;
        color: #999;
    }
    blockquote cite {
        display: block;
        text-align: right;
        color: #666;
        font-size: 1.2rem;
    }
    cite {
      display: block;
      font-size: 0.925rem;
    }
    cite:before {
      content: "\2014 \0020";
    }
    p {
      margin-top: 0;
      color: #74787E;
      font-size: 12pt;
      line-height: 1.5em;
    }
    p.sub {
      font-size: 9pt;
    }
    p.center {
      text-align: center;
    }
    table {
      width: 100%;
    }
    th {
      padding: 0px 5px;
      padding-bottom: 8px;
      border-bottom: 1px solid #EDEFF2;
    }
    th p {
      margin: 0;
      color: #9BA2AB;
      font-size: 9pt;
    }
    td {
      padding: 10px 5px;
      color: #74787E;
      font-size: 11pt;
      line-height: 14pt;
    }
    .content {
      align: center;
      padding: 0;
    }

    .data-wrapper {
      width: 100%;
      margin: 0;
      padding: 26pt 0;
    }
    .data-table {
      width: 100%;
      margin: 0;
    }
    .data-table th {
      text-align: left;
      padding: 0px 5px;
      padding-bottom: 8px;
      border-bottom: 1px solid #EDEFF2;
    }
    .data-table th p {
      margin: 0;
      color: #9BA2AB;
      font-size: 9pt;
    }
    .data-table td {
      padding: 10px 5px;
      color: #74787E;
      font-size: 11pt;
      line-height: 14pt;
    }

    .invite-code {
      display: inline-block;
      padding-top: 15pt;
      padding-right: 27pt;
      padding-bottom: 12pt;
      padding-left: 27pt;
      border-radius: 3px;
      font-family: Consolas, monaco, monospace;
      font-size: 21pt;
      text-align: center;
      letter-spacing: 8px;
      color: #555;
      background-color: #eee;
    }

    .button {
      display: inline-block;
      width: 100%;
      background-color: #00948d;
      color: #ffffff !important;
      font-size: 11pt;
      line-height: 34pt;
      text-align: center;
      text-decoration: none;
      -webkit-text-size-adjust: none;

    }

    @media only screen and (max-width: 600px) {
      .email-body_inner,
      .email-footer {
        width: 100% !important;
      }
    }
  </style>
</head>
